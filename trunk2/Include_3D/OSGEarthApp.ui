   <string notr="true">/* 全局背景和字体 */
QWidget#OSGEarthAppClass {
    background-color: #1a1c1f;
    font-family: &quot;Microsoft YaHei&quot;;
}

/* 1. 左侧工具栏 */
QFrame#toolbarFrame {
    background-color: #25282d;
    border-right: 1px solid #3e4248;
}

#toolbarFrame QPushButton {
    background-color: transparent;
    border: none;
    color: #a0aab4;
    padding: 15px;
    icon-size: 32px 32px;
}

#toolbarFrame QPushButton:hover {
    background-color: #32363c;
    color: white;
}

#toolbarFrame QPushButton:checked {
    background-color: #0078d4;
    color: white;
}

/* 2. 右侧 StackedWidget 面板 */
QStackedWidget {
    background-color: rgba(37, 40, 45, 0.95); /* 稍微透明感 */
    border: 1px solid #3e4248;
border-radius: 4px;
}
QWidget#page {
    background-color: transparent;
}

/* 输入框样式 */
QLineEdit#leProjectName {
    background-color: #1a1c1f;
    border: 1px solid #45494f;
    border-radius: 4px;
    color: black;
    padding: 5px;
}

/* 按钮通用样式 (加载、保存) */
QPushButton#btnLoadScene, QPushButton#btnSavePro {
    background-color: #0078d4;
    color: white;
    border: none;
    border-radius: 4px;
    font-weight: bold;
    height: 35px;
}

QPushButton#btnLoadScene:hover { background-color: #2b88d8; }
QPushButton#btnSavePro:hover { background-color: #2b88d8; }

/* 3. 树形控件样式 */
QTreeWidget#treeSceneManager {
    background-color: #1e2125;
    border: 1px solid #3e4248;
    color: #d1d1d1;
    font-size: 13px;
    outline: none;
}

QTreeWidget#treeSceneManager::item {
    height: 30px;
    border-bottom: 1px solid #2a2e33;
}

QTreeWidget#treeSceneManager::item:selected {
    background-color: #3e4248;
    color: #0078d4;
}

/* 4. 底部文本提示 */
QLabel#lblText {
    color: #888e96;
    font-size: 12px;
}

/* page_2 容器背景（可选，确保背景统一） */
QWidget#page_2 {
    background-color: transparent;
}

/* page_2 内按钮的通用样式 */
#page_2 QPushButton {
    background-color: #3e4248;
    color: #e0e0e0;
    border: 1px solid #45494f;
    border-radius: 4px;
    height: 40px;
    font-size: 14px;
    margin-bottom: 10px; /* 配合垂直布局的间距 */
}

/* 悬浮状态：背景变亮，边框变蓝 */
#page_2 QPushButton:hover {
    background-color: #4e5258;
    border: 1px solid #0078d4;
    color: white;
}

/* 选中/按下状态：经典的 Windows Blue 背景 */
#page_2 QPushButton:pressed, #page_2 QPushButton:checked {
    background-color: #005a9e;
    border: 1px solid #0078d4;
    color: white;
}


/* 假设第三个按钮是重置或清除 */
#btnReset {
    background-color: #443131; /* 稍微带点暗红，提示这是个清空操作 */
}
#btnReset:hover {
    background-color: #a33b3b;
}</string>
